{txt}
{com}. label var coethnic "Coethnic"
{txt}
{com}. * average polity2 score for the 7 years 
. gen Polity = (polity2_7 + polity2_8 + polity2_9 + polity2_10 + polity2_11 + polity2_12 + polity2_13)/7
{txt}(10,811 missing values generated)

{com}. label var Polity "Average polity between the ages of 6 and 13."
{txt}
{com}. 
. * interaction between polity and edu
. gen inter_polity2_coethnic = Polity * coethnic
{txt}(10,811 missing values generated)

{com}. label var inter_polity2_coethnic "\textit{c -(}Polity{c )-}$\times$\textit{c -(}Coethnic Leader{c )-}"
{txt}
{com}. * gen Completed Primary Education Dummy
. gen CompletedPrimaryEdu = 0
{txt}
{com}. label var CompletedPrimaryEdu "Schooling"
{txt}
{com}. replace CompletedPrimaryEdu = 1 if educlvl==1
{txt}(372,124 real changes made)

{com}. replace CompletedPrimaryEdu = 1 if educlvl==2
{txt}(322,681 real changes made)

{com}. replace CompletedPrimaryEdu = 1 if educlvl==3
{txt}(48,818 real changes made)

{com}. ********************************************************************************
. * democratic groups
. gen Democracy = 0
{txt}
{com}. replace Democracy = 1 if Polity<=9 & Polity>=6
{txt}(78,605 real changes made)

{com}. 
. gen Anocracy = 0
{txt}
{com}. replace Anocracy = 1 if Polity>=-5 & Polity<=5
{txt}(538,204 real changes made)

{com}. //
